{
    "manifest_version": 3,
    "name": "CRM",
    "description": "Pra processos administrativos do CRM Dynamics 365.",
    "version": "1.0",
    "action": {
      "default_popup" : "popup/popup.html"
    },
    "icons": {
      "16": "images/crm16.png",
      "32": "images/crm32.png",
      "48": "images/crm48.png",
      "128": "images/crm128.png"
    },
          "permissions": ["activeTab","scripting","webNavigation","storage"]
        }